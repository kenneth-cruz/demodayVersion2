<%- include('header'); -%>
<body>
  <h1>Feed</h1>
  <ul>
  <% for(var i=0; i<posts.length; i++) {%>
    <li>
      <%# the source here for the image is pulling from the collection posts. %>
      <a href="/post/<%= posts[i]._id%>">
        <img src="<%= posts[i].imgPath%>">
      </a>
      <span><%= posts[i].caption %></span>
      <span><%= posts[i].likes %></span>
      <span><i class="fa fa-heart" aria-hidden="true"></i></span>
    </li>
  <% } %>
  </ul>
<%- include('footer'); -%>
